        "distribute-gas",
        "--network",
        "localhost",
        "--batch-size",
        "3",
        "--max-retries",
        "3",
        "--delay-ms",
        "3000"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
